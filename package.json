{
  "name": "german-verbs-db",
  "version": "1.0.0",
  "description": "German verb conjugation database populated from Wiktionary",
  "main": "src/index.js",
  "type": "module",
  "files": [
    "schema.sql",
    "german-verbs.db"
  ],
  "scripts": {
    "test": "node --test",
    "scrape": "node src/index.js scrape",
    "scrape:resume": "node src/index.js scrape --resume",
